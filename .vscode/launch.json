    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "MockExpectationLoader",
            "request": "launch",
            "mainClass": "com.example.kitespringapp.util.MockExpectationLoader",
            "projectName": "kitespringapp"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "KiteSpringAppApplication",
            "request": "launch",
            "mainClass": "com.example.kitespringapp.KiteSpringAppApplication",
            "projectName": "kitespringapp",
            "args": "--spring.profiles.active=mock"
        }
    ]
